<form method="POST" action="/customers/<%= customerId %>/?_method=PUT">
    <label>Phone:</label>
    <input type="text" value="<%= customer.phone %>" name="phone" />
    <label>First Name:</label>
    <input type="text" value="<%= customer.firstName %>" name="firstName" />
    <label>Last Name:</label>
    <input type="text" value="<%= customer.lastName %>" name="lastName" />
    <!-- < % customer.locations.forEach(location => { %>}) -->
    <label>Address:</label>
    <input type="text" value="<%= location.address %>" name="address" />
  
    <input type="submit" />
  </form>